<?xml version="1.0" ?>
<LocomotionController>
	<Mission>
		<Configuration>
			<Position>
				<Initial x="0.0" y="0.0" z="0.0" />
				<Minimal x="-0.0" y="-0.0" z="0.0" />
				<Maximal x="0.0" y="0." z="0.0" />
			</Position>
			<Orientation><!-- Euler Angle ZYX -->
				<Initial x="0.0" y="0.0" z="0" />
				<Minimal x="-0.0" y="-0.0" z="-0.0" />
				<Maximal x="0.0" y="0.0" z="0.0" />
			</Orientation>
		</Configuration>
		<Speed>
			<Initial headingSpeed="0.0" lateralSpeed="0.0" turningSpeed="0.0" />
			<Maximum headingSpeed="0.2" lateralSpeed="0.2" turningSpeed="0.8" />
		</Speed>
	</Mission>
	<LimbCoordination>
		<GaitPattern> 
			<APS>
	             <GaitDiagram cycleDuration="1.5" foreLag="0.5" hindLag="0.5" pairLag="0.75" foreDutyFactor="0.84" hindDutyFactor="0.84" />
	        </APS>
	        <FlightPhases cycleDuration="3.0" initCyclePhase="0.5">
		        <LF liftOff="0.02" touchDown="0.25" />
				<RF liftOff="0.52" touchDown="0.75" />
				<LH liftOff="0.77" touchDown="1.00" />
				<RH liftOff="0.27" touchDown="0.5" />
	        </FlightPhases>
		</GaitPattern>
	</LimbCoordination>
	<TorsoControl>
		<TorsoConfiguration>
			<TorsoHeight torsoHeight="0.40" />
		</TorsoConfiguration>
		<DynamicGait>
			<CoMOverSupportPolygonControl>
				<Weight minSwingLegWeight="0.55" />
				<Timing startShiftAwayFromLegAtStancePhase="0.5" startShiftTowardsLegAtSwingPhase="0.5" />
			</CoMOverSupportPolygonControl>
			<HipConfiguration>
				<HeightTrajectory offset="0.39" fore="true" hind="true">
					<Knot t="0.00" v="-0" />
					<Knot t="0.25" v="0.00" />
					<Knot t="0.50" v="-0.0" />
					<Knot t="0.75" v="0.00" />
					<Knot t="1.00" v="-0" />
				</HeightTrajectory>
			</HipConfiguration>
		</DynamicGait>
	</TorsoControl>
	<FootPlacementStrategy>
		<InvertedPendulum>
			<Offset>
				<Fore heading="-0.00" lateral="-0.01" />
				<Hind  heading="0.00" lateral="0.1" />
			</Offset>
			<HeightTrajectory>
				<Knot t="0.0" v="0.0" />
				<Knot t="0.5" v="0.07" />
				<Knot t="1.0" v="-0.00" />
			</HeightTrajectory>
	        <Gains feedbackScale="1.1" />
        </InvertedPendulum>
	</FootPlacementStrategy>
	<VirtualModelController>
		<Gains>
			<Heading 	kp="600" 	kd="50" 	kff="25.0"	/>
			<Lateral 	kp="800" 	kd="100"	kff="0"		/>
			<Vertical 	kp="600" 	kd="120" 	kff="0"/>
			<Roll 		kp="62" 	kd="5" 		kff="0" />
			<Pitch 		kp="60" 	kd="4" 		kff="0" />
			<Yaw 		kp="50" 		kd="20" 	kff="0" /> 	/>
		</Gains>
	</VirtualModelController>
	<ContactForceDistribution>
		<Weights>
			<Force heading="1.0" lateral="1.0" vertical="1" />
			<Torque roll="10" pitch="10" yaw="5" />
			<Regularizer value="0.00001"/>
		</Weights>
		<Constraints frictionCoefficient="0.7" minimalNormalForce="2.0" />
		<LoadFactor loadFactor="1.0" />
	</ContactForceDistribution> 
</LocomotionController>

